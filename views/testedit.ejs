                                            <input id="lineHeight" type="range" max="5" value="1" min="-2" step="1">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div style="clear: both;"></div>

                            <div class="footer icons mt-2">
                                <div class="contain">
                                    <div class="row">
                                        <button type="button" id="bold" data-value="Bold" class="btn">
                                            <span><i class="uil uil-bold"></i></span>
                                        </button>
                                        <button type="button" id="italic" data-value="italic" class="btn">
                                            <span><i class="uil uil-italic"></i></span>
